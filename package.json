{
  "name": "@sanity/codegen",
  "version": "5.2.0",
  "description": "Codegen toolkit for Sanity.io",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "codegen"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/codegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/codegen.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "babel.config.json"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --check --clean",
    "check:types": "tsgo --project tsconfig.lib.json --erasableSyntaxOnly",
    "clean": "rimraf lib coverage",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "watch": "pkg-utils watch"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/generator": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@sanity/worker-channels": "^1.1.0",
    "debug": "^4.4.3",
    "globby": "^11.1.0",
    "groq": "^5.2.0",
    "groq-js": "^1.25.0",
    "json5": "^2.2.3",
    "reselect": "^5.1.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@sanity/eslint-config-cli": "0.0.0-alpha.1",
    "@sanity/pkg-utils": "^10.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__register": "^7.17.3",
    "@types/babel__traverse": "^7.28.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.19.28",
    "@typescript/native-preview": "7.0.0-dev.20260111.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "rimraf": "^5.0.10",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
